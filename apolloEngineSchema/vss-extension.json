{
    "manifestVersion": 1,
    "id": "vsts-extensions-apollo-engine-schema",
    "name": "Apollo Engine Schema",
    "version": "0.0.6",
    "publisher": "jyc",
    "public": true,
    "galleryFlags": [
        "Preview"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Apollo Engine schema embedded in VSTS!",
    "categories": [
        "Build and release"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "tags": [
        "apollo",
        "graphQL"
    ],
    "content": {
        "details": {
            "path": "apolloEngineSchema/README.md"
        }
    },
    "links": {
        "repository": {
            "uri": "https://github.com/jycouet/VSTSExtensions"
        },
        "issues": {
            "uri": "https://github.com/jycouet/VSTSExtensions/issues"
        }
    },
    "repository": {
		"type": "git",
		"uri":	"https://github.com/jycouet/VSTSExtensions"
	},
    "branding": {
        "color": "#55B541",
        "theme": "dark"
    },
    "files": [
        {
            "path": "buildtask"
        }
    ],
    "contributions": [
        {
            "id": "default-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "apolloEngineSchema/buildtask"
            }
        }
    ]
}